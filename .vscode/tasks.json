{
	"version": "2.0.0",
	"tasks": [
	  {
		"label": "Build Orderbook",
		"type": "shell",
		"command": "clang++",
		"args": [
		  "-std=c++20",
		  "-I/opt/homebrew/include",
		  "-L/opt/homebrew/lib",
		  "orderbook.cpp",
		  "-lfmt",
		  "-o",
		  "orderbook"
		],
		"group": {
		  "kind": "build",
		  "isDefault": true
		},
		"problemMatcher": ["$gcc"]
	  }
	]
  }
  